FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/solid1";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 1;

boundaryField
{
    atmosphere
    {
        type            zeroGradient;
    }
    solid1_to_solid
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
}


