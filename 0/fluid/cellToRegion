FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 2;

boundaryField
{
    boundSymmetry
    {
        type            symmetry;
    }
    inlet
    {
        type            zeroGradient;
    }
    fluid_to_solid
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
}


