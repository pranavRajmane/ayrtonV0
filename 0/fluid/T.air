FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      T.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    boundSymmetry
    {
        type            symmetry;
    }
    inlet
    {
        type            calculated;
        value           uniform 1673;
    }
    fluid_to_solid
    {
        type            calculated;
        value           uniform 300;
    }
}


