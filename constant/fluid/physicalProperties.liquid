thermoType 
{     
    type            heRhoThermo;     
    mixture         pureMixture;     
    transport       const;     
    thermo          eConst;     
    equationOfState rhoConst;     
    specie          specie;     
    energy          sensibleInternalEnergy; 
}  

mixture 
{     
    specie     
    {         
        molWeight   55.85;     
    }     
    equationOfState     
    {         
        rho         7300;     
    }     
    thermodynamics     
    {         
        Cv          542.85;         
        Hf          0;     
    }     
    transport     
    {         
    	mu          0.006; 
    	Pr	    0.108;	
    } 
}

