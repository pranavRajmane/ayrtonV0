regionSolvers
{
    fluid           compressibleVoF;
    solid           compressibleVoF;
}

startTime       0;

stopAt          endTime;

endTime         100;

deltaT          1e-5;

writeControl    timeStep;

writeInterval   100;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;    

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.6;

maxAlphaCo      0.6;

maxDeltaT       0.01;
